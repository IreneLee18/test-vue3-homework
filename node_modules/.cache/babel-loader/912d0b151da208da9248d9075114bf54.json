l","deleteCoupon","created"],"sourceRoot":"","sources":["/Users/irenelee/Desktop/vue-cli-demo/project/test1/src/views/CouponsView.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- <Loading :active=\"isLoading\"></Loading> -->\n    <div class=\"text-end mt-4\">\n      <button class=\"btn btn-primary\"\n        @click=\"openModal(true)\"\n      >\n        建立新的優惠券\n      </button>\n    </div>\n    <table class=\"table mt-4\">\n      <thead>\n      <tr>\n        <th>名稱</th>\n        <th>折扣百分比</th>\n        <th>到期日</th>\n        <th>是否啟用</th>\n        <th>編輯</th>\n      </tr>\n      </thead>\n      <tbody>\n      <tr v-for=\"item in coupons\" :key=\"item.id\">\n        <td>{{ item.code }}</td>\n        <td>{{ item.percent }}%</td>\n        <td>{{ $filters.date(item.due_date) }}</td>\n        <td>\n          <span v-if=\"item.is_enabled === 1\" class=\"text-success\">啟用</span>\n          <span v-else class=\"text-muted\">未起用</span>\n        </td>\n        <td>\n          <div class=\"btn-group\">\n            <button class=\"btn btn-outline-primary btn-sm\"\n            @click=\"openModal(false,item)\"\n            >編輯</button>\n            <button class=\"btn btn-outline-danger btn-sm\"\n            @click=\"deleteCoupon(item)\"\n            >刪除</button>\n          </div>\n        </td>\n      </tr>\n      </tbody>\n    </table>\n  </div>\n  <coupon-modal ref=\"showCouponModal\"\n  :coupon=\"tempCoupon\"\n  @add-coupon=\"updateCoupon\"\n  ></coupon-modal>\n  <!-- <delete-modal ref=\"showDeleteModal\"\n  :coupon=\"tempCoupon\"\n  @delete-item=\"deleteCoupon(item)\"></delete-modal> -->\n</template>\n\n<script>\nimport couponModal from '../components/CouponModal.vue'\n// import deleteModal from '../components/DeleteModal.vue'\nexport default {\n  components: {\n    couponModal\n    // deleteModal\n  },\n  data () {\n    return {\n      coupons: [],\n      tempCoupon: {\n        title: '',\n        is_enabled: 0,\n        percent: 100,\n        code: ''\n      },\n      isNew: false,\n      isLoading: false\n    }\n  },\n  methods: {\n    getCoupons () {\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/coupons`\n      // console.log(api)\n      this.$http.get(api)\n        .then((res) => {\n          if (res.data.success) {\n            this.isLoading = false\n            console.log(res.data)\n            this.coupons = res.data.coupons\n          }\n        })\n        .catch((err) => console.log(err.response))\n    },\n    updateCoupon (item) {\n      this.tempCoupon = item\n      // add\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/coupon`\n      this.$http.post(api, { data: this.tempCoupon })\n        .then(res => {\n          console.log(res)\n        })\n        .catch(err => console.log(err.response))\n      this.$refs.showCouponModal.hideModal()\n    },\n    openModal (isNew, item) {\n      this.isNew = isNew\n      if (this.isNew) {\n        this.tempCoupon = {\n          due_date: new Date().getTime()\n          // 日期轉換格式-老師寫法\n          // due_date: new Date().getTime() / 1000\n        }\n      } else {\n        this.tempCoupon = { ...item }\n      }\n      this.$refs.showCouponModal.showModal()\n    },\n    deleteCoupon (item) {\n      // this.coupons = { ...item }\n      // const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/coupon/${this.coupons.id}`\n      // this.$http.delete(api)\n      console.log(item)\n    }\n  },\n  created () {\n    this.getCoupons()\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}