{"ast":null,"code":"import productModal from '../components/ProductModal.vue';\nimport deleteModal from '../components/DeleteModal.vue';\nexport default {\n  data() {\n    return {\n      products: [],\n      pagination: {},\n      tempProduct: {},\n      isNewProduct: false\n    };\n  },\n\n  components: {\n    productModal,\n    deleteModal\n  },\n  methods: {\n    getProducts() {\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/products`;\n      console.log(api);\n      this.$http.get(api).then(res => {\n        if (res.data.success) {\n          console.log(res.data);\n          this.products = res.data.products;\n          this.pagination = res.data.pagination;\n          console.log(this.products, this.pagination);\n        }\n      }).catch(err => console.log(err.response));\n    },\n\n    // 開啟新增＆編輯＆刪除\n    openModal(isNewProduct, item) {\n      // console.log(isNewProduct, item)\n      if (isNewProduct) {\n        // 每次都清空資料欄\n        this.tempProduct = {};\n      } else {\n        this.tempProduct = { ...item\n        };\n      }\n\n      this.isNewProduct = isNewProduct;\n      const productModalComponent = this.$refs.showProductModal;\n      productModalComponent.showModal();\n    },\n\n    updateProduct(item) {\n      this.tempProduct = item; // 新增狀態\n\n      let api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/product`;\n      let httpMethod = 'post'; // 編輯狀態\n\n      if (!this.isNewProduct) {\n        api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/product/${item.id}`;\n        httpMethod = 'put';\n      }\n\n      const productModalComponent = this.$refs.showProductModal;\n      this.$http[httpMethod](api, {\n        data: this.tempProduct\n      }).then(res => {\n        console.log(res);\n        productModalComponent.hideModal();\n        this.getProducts();\n      }).catch(err => console.log(err.response));\n    },\n\n    deleteModal() {\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/product/${item.id}`;\n      this.$http.delete(api, {\n        data: this.tempProduct\n      }).then(res => {\n        console.log(res);\n        deleteModalComponent.hideModal();\n        this.getProducts();\n      }).catch(err => console.log(err.response));\n      const deleteModalComponent = this.$refs.showDeleteModal;\n      console.log('clickDelete');\n    }\n\n  },\n\n  created() {\n    this.getProducts();\n  }\n\n};","map":{"version":3,"mappings":"AAuDA,OAAOA,YAAP,MAAyB,gCAAzB;AACA,OAAOC,WAAP,MAAwB,+BAAxB;AACA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,UAAU,EAAE,EAFP;MAGLC,WAAW,EAAE,EAHR;MAILC,YAAY,EAAE;IAJT,CAAP;EAMD,CARY;;EASbC,UAAU,EAAE;IACVP,YADU;IAEVC;EAFU,CATC;EAabO,OAAO,EAAE;IACPC,WAAU,GAAK;MACb,MAAMC,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAR,CAAYC,WAAY,OAAMF,OAAO,CAACC,GAAR,CAAYE,YAAa,iBAAtE;MACAC,OAAO,CAACC,GAAR,CAAYN,GAAZ;MACA,KAAKO,KAAL,CAAWC,GAAX,CAAeR,GAAf,EACGS,IADH,CACSC,GAAD,IAAS;QACb,IAAIA,GAAG,CAAClB,IAAJ,CAASmB,OAAb,EAAsB;UACpBN,OAAO,CAACC,GAAR,CAAYI,GAAG,CAAClB,IAAhB;UACA,KAAKC,QAAL,GAAgBiB,GAAG,CAAClB,IAAJ,CAASC,QAAzB;UACA,KAAKC,UAAL,GAAkBgB,GAAG,CAAClB,IAAJ,CAASE,UAA3B;UACAW,OAAO,CAACC,GAAR,CAAY,KAAKb,QAAjB,EAA2B,KAAKC,UAAhC;QACF;MACD,CARH,EASGkB,KATH,CASUC,GAAD,IAASR,OAAO,CAACC,GAAR,CAAYO,GAAG,CAACC,QAAhB,CATlB;IAUD,CAdM;;IAeP;IACAC,SAAQ,CAAGnB,YAAH,EAAiBoB,IAAjB,EAAuB;MAC7B;MACA,IAAIpB,YAAJ,EAAkB;QAChB;QACA,KAAKD,WAAL,GAAmB,EAAnB;MACF,CAHA,MAGO;QACL,KAAKA,WAAL,GAAmB,EAAE,GAAGqB;QAAL,CAAnB;MACF;;MACA,KAAKpB,YAAL,GAAoBA,YAApB;MACA,MAAMqB,qBAAoB,GAAI,KAAKC,KAAL,CAAWC,gBAAzC;MACAF,qBAAqB,CAACG,SAAtB;IACD,CA3BM;;IA4BPC,aAAY,CAAGL,IAAH,EAAS;MACnB,KAAKrB,WAAL,GAAmBqB,IAAnB,CADmB,CAGnB;;MACA,IAAIhB,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAR,CAAYC,WAAY,OAAMF,OAAO,CAACC,GAAR,CAAYE,YAAa,gBAApE;MACA,IAAIkB,UAAS,GAAI,MAAjB,CALmB,CAOnB;;MACA,IAAI,CAAC,KAAK1B,YAAV,EAAwB;QACtBI,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAR,CAAYC,WAAY,OAAMF,OAAO,CAACC,GAAR,CAAYE,YAAa,kBAAiBY,IAAI,CAACO,EAAG,EAAzF;QACAD,UAAS,GAAI,KAAb;MA