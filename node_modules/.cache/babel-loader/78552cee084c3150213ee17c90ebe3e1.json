{"ast":null,"code":"import productModal from '../components/ProductModal.vue';\nexport default {\n  data() {\n    return {\n      products: [],\n      pagination: {}\n    };\n  },\n\n  components: {\n    productModal\n  },\n  methods: {\n    getProducts() {\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/products`;\n      console.log(api);\n      this.$http.get(api).then(res => {\n        if (res.data.success) {\n          console.log(res.data);\n          this.products = res.data.products;\n          this.pagination = res.data.pagination;\n          console.log(this.products, this.pagination);\n        }\n      }).catch(err => console.log(err.response));\n    }\n\n  },\n\n  created() {\n    this.getProducts();\n  }\n\n};","map":{"version":3,"mappings":"AA4CA,OAAOA,YAAP,MAAyB,gCAAzB;AACA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,UAAU,EAAE;IAFP,CAAP;EAID,CANY;;EAObC,UAAU,EAAE;IACVJ;EADU,CAPC;EAUbK,OAAO,EAAE;IACPC,WAAU,GAAK;MACb,MAAMC,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAR,CAAYC,WAAY,OAAMF,OAAO,CAACC,GAAR,CAAYE,YAAa,iBAAtE;MACAC,OAAO,CAACC,GAAR,CAAYN,GAAZ;MACA,KAAKO,KAAL,CAAWC,GAAX,CAAeR,GAAf,EACGS,IADH,CACSC,GAAD,IAAS;QACb,IAAIA,GAAG,CAAChB,IAAJ,CAASiB,OAAb,EAAsB;UACpBN,OAAO,CAACC,GAAR,CAAYI,GAAG,CAAChB,IAAhB;UACA,KAAKC,QAAL,GAAgBe,GAAG,CAAChB,IAAJ,CAASC,QAAzB;UACA,KAAKC,UAAL,GAAkBc,GAAG,CAAChB,IAAJ,CAASE,UAA3B;UACAS,OAAO,CAACC,GAAR,CAAY,KAAKX,QAAjB,EAA2B,KAAKC,UAAhC;QACF;MACD,CARH,EASGgB,KATH,CASUC,GAAD,IAASR,OAAO,CAACC,GAAR,CAAYO,GAAG,CAACC,QAAhB,CATlB;IAUF;;EAdO,CAVI;;EA0BbC,OAAM,GAAK;IACT,KAAKhB,WAAL;EACF;;AA5Ba,CAAf","names":["productModal","data","products","pagination","components","methods","getProducts","api","process","env","VUE_APP_API","VUE_APP_PATH","console","log","$http","get","then","res","success","catch","err","response","created"],"sourceRoot":"","sources":["/Users/irenelee/Desktop/vue-cli-demo/project/test1/src/views/ProductsView.vue"],"sourcesContent":["<template>\n  <div class=\"text-end\">\n    <button class=\"btn btn-primary\">\n      新增產品\n    </button>\n  </div>\n  <table class=\"table mt-4\">\n    <thead>\n      <tr>\n        <th width=\"120\">分類</th>\n        <th>產品名稱</th>\n        <th width=\"120\">原價</th>\n        <th width=\"120\">售價</th>\n        <th width=\"100\">是否啟用</th>\n        <th width=\"200\">編輯</th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr v-for=\"item in products\" :key=\"item.id\">\n        <td>{{item.category}}</td>\n        <td>{{item.title}}</td>\n        <td class=\"text-right\">\n          {{item.origin_price}}\n        </td>\n        <td class=\"text-right\">\n          {{item.price}}\n        </td>\n        <td>\n          <span class=\"text-success\" v-if=\"item.is_enabled\">啟用</span>\n          <span class=\"text-muted\" v-else>未啟用</span>\n        </td>\n        <td>\n          <div class=\"btn-group\">\n            <button class=\"btn btn-outline-primary btn-sm\">編輯</button>\n            <button class=\"btn btn-outline-danger btn-sm\">刪除</button>\n          </div>\n        </td>\n      </tr>\n    </tbody>\n  </table>\n  <product-modal></product-modal>\n</template>\n\n<script>\nimport productModal from '../components/ProductModal.vue'\nexport default {\n  data () {\n    return {\n      products: [],\n      pagination: {}\n    }\n  },\n  components: {\n    productModal\n  },\n  methods: {\n    getProducts () {\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/products`\n      console.log(api)\n      this.$http.get(api)\n        .then((res) => {\n          if (res.data.success) {\n            console.log(res.data)\n            this.products = res.data.products\n            this.pagination = res.data.pagination\n            console.log(this.products, this.pagination)\n          }\n        })\n        .catch((err) => console.log(err.response))\n    }\n  },\n  created () {\n    this.getProducts()\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}