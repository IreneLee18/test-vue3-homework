{"ast":null,"code":"import productModal from '../components/ProductModal.vue';\nexport default {\n  data() {\n    return {\n      products: [],\n      tempProduct: {},\n      pagination: {}\n    };\n  },\n\n  components: {\n    productModal\n  },\n  methods: {\n    getProducts() {\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/products`;\n      console.log(api);\n      this.$http.get(api).then(res => {\n        if (res.data.success) {\n          console.log(res.data);\n          this.products = res.data.products;\n          this.pagination = res.data.pagination;\n          console.log(this.products, this.pagination);\n        }\n      }).catch(err => console.log(err.response));\n    },\n\n    openModal() {\n      this.tempProduct = {};\n      const productModalComponent = this.$refs.showProductModal;\n      productModalComponent.showModal();\n    }\n\n  },\n\n  created() {\n    this.getProducts();\n  }\n\n};","map":{"version":3,"mappings":"AA4CA,OAAOA,YAAP,MAAyB,gCAAzB;AACA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,WAAW,EAAE,EAFR;MAGLC,UAAU,EAAE;IAHP,CAAP;EAKD,CAPY;;EAQbC,UAAU,EAAE;IACVL;EADU,CARC;EAWbM,OAAO,EAAE;IACPC,WAAU,GAAK;MACb,MAAMC,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAR,CAAYC,WAAY,OAAMF,OAAO,CAACC,GAAR,CAAYE,YAAa,iBAAtE;MACAC,OAAO,CAACC,GAAR,CAAYN,GAAZ;MACA,KAAKO,KAAL,CAAWC,GAAX,CAAeR,GAAf,EACGS,IADH,CACSC,GAAD,IAAS;QACb,IAAIA,GAAG,CAACjB,IAAJ,CAASkB,OAAb,EAAsB;UACpBN,OAAO,CAACC,GAAR,CAAYI,GAAG,CAACjB,IAAhB;UACA,KAAKC,QAAL,GAAgBgB,GAAG,CAACjB,IAAJ,CAASC,QAAzB;UACA,KAAKE,UAAL,GAAkBc,GAAG,CAACjB,IAAJ,CAASG,UAA3B;UACAS,OAAO,CAACC,GAAR,CAAY,KAAKZ,QAAjB,EAA2B,KAAKE,UAAhC;QACF;MACD,CARH,EASGgB,KATH,CASUC,GAAD,IAASR,OAAO,CAACC,GAAR,CAAYO,GAAG,CAACC,QAAhB,CATlB;IAUD,CAdM;;IAePC,SAAQ,GAAK;MACX,KAAKpB,WAAL,GAAmB,EAAnB;MACA,MAAMqB,qBAAoB,GAAI,KAAKC,KAAL,CAAWC,gBAAzC;MACAF,qBAAqB,CAACG,SAAtB;IACF;;EAnBO,CAXI;;EAgCbC,OAAM,GAAK;IACT,KAAKrB,WAAL;EACF;;AAlCa,CAAf","names":["productModal","data","products","tempProduct","paginati