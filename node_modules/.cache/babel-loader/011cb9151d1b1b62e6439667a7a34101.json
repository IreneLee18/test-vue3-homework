{"ast":null,"code":"import couponModal from '../components/CouponModal.vue';\nimport deleteModal from '../components/DeleteModal.vue';\nexport default {\n  components: {\n    couponModal,\n    deleteModal\n  },\n\n  data() {\n    return {\n      coupons: [],\n      tempCoupon: {\n        title: '',\n        is_enabled: 0,\n        percent: 100,\n        code: '',\n        due_date: ''\n      },\n      isNew: false,\n      isLoading: false\n    };\n  },\n\n  methods: {\n    getCoupons() {\n      this.isLoading = true;\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/coupons`; // console.log(api)\n\n      this.$http.get(api).then(res => {\n        this.isLoading = false;\n        this.coupons = res.data.coupons;\n        console.log(res.data);\n      }).catch(err => console.log(err.response));\n    },\n\n    updateCoupon(item) {\n      this.tempCoupon = item;\n      console.log(this.tempCoupon); // 新增\n\n      if (this.isNew) {\n        const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/coupon`;\n        this.$http.post(api, {\n          data: this.tempCoupon\n        }).then(res => {\n          console.log(res, this.tempCoupon);\n        }).catch(err => console.log(err.response));\n        this.getCoupons();\n        this.$refs.showCouponModal.hideModal();\n      } else {\n        // 編輯\n        const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/coupon/${this.tempCoupon.id}`;\n        this.$http.put(api, {\n          data: this.tempCoupon\n        }).then(res => {\n          console.log(res);\n        }).catch(err => console.log(err.response));\n        this.getCoupons();\n        this.$refs.showCouponModal.hideModal();\n      }\n    },\n\n    openModal(isNew, item) {\n      this.isNew = isNew;\n\n      if (this.isNew) {\n        this.tempCoupon = {\n          due_date: new Date().getTime() // 日期轉換格式-老師寫法\n          // due_date: new Date().getTime() / 1000\n\n        };\n      } else {\n        this.tempCoupon = { ...item\n        };\n      }\n\n      this.$refs.showCouponModal.showModal();\n    },\n\n    openDeleteModal(item) {\n      this.tempCoupon = { ...item\n      };\n      this.$refs.showDeleteModal.showModal();\n    },\n\n    deleteCoupon() {\n      console.log(this.tempCoupon.id);\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/coupon/${this.tempCoupon.id}`;\n      this.$http.delete(api).then(res => console.log(res)).catch(err => console.log(err.response));\n      this.getCoupons();\n      this.$refs.showDeleteModal.hideModal();\n    }\n\n  },\n\n  created() {\n    this.getCoupons();\n  }\n\n};","map":{"version":3,"mappings":"AAwDA,OAAOA,WAAP,MAAwB,+BAAxB;AACA,OAAOC,WAAP,MAAwB,+BAAxB;AACA,eAAe;EACbC,UAAU,EAAE;IACVF,WADU;IAEVC;EAFU,CADC;;EAKbE,IAAG,GAAK;IACN,OAAO;MACLC,OAAO,EAAE,EADJ;MAELC,UAAU,EAAE;QACVC,KAAK,EAAE,EADG;QAEVC,UAAU,EAAE,CAFF;QAGVC,OAAO,EAAE,GAHC;QAIVC,IAAI,EAAE,EAJI;QAKVC,QAAQ,EAAE;MALA,CAFP;MASLC,KAAK,EAAE,KATF;MAULC,SAAS,EAAE;IAVN,CAAP;EAYD,CAlBY;;EAmBbC,OAAO,EAAE;IACPC,UAAS,GAAK;MACZ,KAAKF,SAAL,GAAiB,IAAjB;MACA,MAAMG,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAR,CAAYC,WAAY,OAAMF,OAAO,CAACC,GAAR,CAAYE,YAAa,gBAAtE,CAFY,CAGZ;;MACA,KAAKC,KAAL,CAAWC,GAAX,CAAeN,GAAf,EACGO,IADH,CACSC,GAAD,IAAS;QACb,KAAKX,SAAL,GAAiB,KAAjB;QACA,KAAKR,OAAL,GAAemB,GAAG,CAACpB,IAAJ,CAASC,OAAxB;QACAoB,OAAO,CAACC,GAAR,CAAYF,GAAG,CAACpB,IAAhB;MACD,CALH,EAMGuB,KANH,CAMUC,GAAD,IAASH,OAAO,CAACC,GAAR,CAAYE,GAAG,CAACC,QAAhB,CANlB;IAOD,CAZM;;IAaPC,YAAW,CAAGC,IAAH,EAAS;MAClB,KAAKzB,UAAL,GAAkByB,IAAlB;MACAN,OAAO,CAACC,GAAR,CAAY,KAAKpB,UAAjB,EAFkB,CAGlB;;MACA,IAAI,KAAKM,KAAT,EAAgB;QACd,MAAMI,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAR,CAAYC,WAAY,OAAMF,OAAO,CAACC,GAAR,CAAYE,YAAa,eAAtE;QACA,KAAKC,KAAL,CAAWW,IAAX,CAAgBhB,GAAhB,EAAqB;UAAEZ,IAAI,EAAE,KAAKE;QAAb,CAArB,EACGiB,IADH,CACQC,GAAE,IAAK;UACXC,OAAO,CAACC,GAAR,CAAYF,GAAZ,EAAiB,KAAKlB,UAAtB;QACD,CAHH,EAIGqB,KAJH,CAISC,GAAE,IAAKH,OAAO,CAACC,GAAR,CAAYE,GAAG,CAACC,QAAhB,CAJhB;QAKA,KAAKd,UAAL;QACA,KA