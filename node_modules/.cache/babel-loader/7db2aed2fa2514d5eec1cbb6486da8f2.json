{"ast":null,"code":"export default {\n  data() {\n    return {\n      user: {\n        username: '',\n        password: ''\n      }\n    };\n  },\n\n  methods: {\n    signIn() {\n      const api = `${process.env.VUE_APP_API}/admin/signin`;\n      console.log(api);\n      this.$http.post(api, this.user).then(res => {\n        if (res.data.success) {\n          const {\n            token,\n            expired\n          } = res.data;\n          document.cookie = `myToken=${token};expires=${new Date(expired)}`; // console.log(token, expired, new Date(expired)) // 在設定之前可以先看是否有取得到資料\n\n          this.$router.push('/stilllogin');\n        }\n\n        console.log(res);\n      }).catch(err => console.log(err.response));\n    }\n\n  } // mounted () {\n  //   this.api = `${process.env.VUE_APP_API}/admin/signin`\n  // }\n\n};","map":{"version":3,"mappings":"AAwCA,eAAe;EACbA,IAAG,GAAK;IACN,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EADN;QAEJC,QAAQ,EAAE;MAFN;IADD,CAAP;EAMD,CARY;;EASbC,OAAO,EAAE;IACPC,MAAK,GAAK;MACR,MAAMC,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAR,CAAYC,WAAY,eAAvC;MACAC,OAAO,CAACC,GAAR,CAAYL,GAAZ;MACA,KAAKM,KAAL,CAAWC,IAAX,CAAgBP,GAAhB,EAAqB,KAAKL,IAA1B,EACGa,IADH,CACSC,GAAD,IAAS;QACb,IAAIA,GAAG,CAACf,IAAJ,CAASgB,OAAb,EAAsB;UACpB,MAAM;YAAEC,KAAF;YAASC;UAAT,IAAqBH,GAAG,CAACf,IAA/B;UACAmB,QAAQ,CAACC,MAAT,GAAmB,WAAUH,KAAM,YAAW,IAAII,IAAJ,CAASH,OAAT,CAAkB,EAAhE,CAFoB,CAGpB;;UACA,KAAKI,OAAL,CAAaC,IAAb,CAAkB,aAAlB;QACF;;QACAb,OAAO,CAACC,GAAR,CAAYI,GAAZ;MACD,CATH,EAUGS,KAVH,CAUUC,GAAD,IAASf,OAAO,CAACC,GAAR,CAAYc,GAAG,CAACC,QAAhB,CAVlB;IAWF;;EAfO,CATI,CA0Bb;EACA;EACA;;AA5Ba,CAAf","names":["data","user","username","password","methods","signIn","api","process","env","VUE_APP_API","console","log","$http","post","then","res","success","token","expired","document","cookie","Date","$router","push","catch","err","response"],"sourceRoot":"","sources":["/Users/irenelee/Desktop/vue-cli-demo/project/test1/src/views/LoginView.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <form class=\"row justify-content-center\" @submit.prevent=\"signIn\">\n      <div class=\"col-md-6 border\">\n        <h1 class=\"h3 mb-3 font-weight-normal\">請先登入</h1>\n        <div class=\"mb-2\">\n          <label for=\"inputEmail\" class=\"sr-only\">信箱</label>\n          <input\n            type=\"email\"\n            id=\"inputEmail\"\n            class=\"form-control\"\n            placeholder=\"Email address\"\n            required\n            autofocus\n            v-model=\"user.username\"\n          />\n        </div>\n        <div class=\"mb-2\">\n          <label for=\"inputPassword\" class=\"sr-only\">密碼</label>\n          <input\n            type=\"password\"\n            id=\"inputPassword\"\n            class=\"form-control\"\n            placeholder=\"Password\"\n            required\n            v-model=\"user.password\"\n          />\n        </div>\n\n        <div class=\"text-end mt-4\">\n          <button class=\"btn btn-lg btn-primary btn-block\" type=\"submit\">\n            登入\n          </button>\n        </div>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nexport default {\n  data () {\n    return {\n      user: {\n        username: '',\n        password: ''\n      }\n    }\n  },\n  methods: {\n    signIn () {\n      const api = `${process.env.VUE_APP_API}/admin/signin`\n      console.log(api)\n      this.$http.post(api, this.user)\n        .then((res) => {\n          if (res.data.success) {\n            const { token, expired } = res.data\n            document.cookie = `myToken=${token};expires=${new Date(expired)}`\n            // console.log(token, expired, new Date(expired)) // 在設定之前可以先看是否有取得到資料\n            this.$router.push('/stilllogin')\n          }\n          console.log(res)\n        })\n        .catch((err) => console.log(err.response))\n    }\n  }\n  // mounted () {\n  //   this.api = `${process.env.VUE_APP_API}/admin/signin`\n  // }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}